image/*; xdg-open %s
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump '%s'; copiousoutput; description=HTML Text; nametemplate=%s.html
application/msword;		antiword  %s; copiousoutput
application/vnd.msword;		antiword  %s; copiousoutput
application/vnd.openxmlformats-officedocument.wordprocessingml.document; docx2txt %s -; copiousoutput
application/vnd.oasis.opendocument.text; odt2txt %s -; copiousoutput
application/pdf; zathura %s
